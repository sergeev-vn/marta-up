.specialised {
  padding: 55px 0 93px;
  background-image: url(~images/icons/specialised-bg.svg);
  background-repeat: no-repeat;
  background-position: left bottom;
}

.specialised__title {
  margin-bottom: 96px;
}

.specialised__list {
  display: flex;
}

.specialised__item-title {
  font-weight: bold;
  font-size: 20px;
  line-height: 24px;
}

.specialised__item-text {
  font-size: 15px;
  line-height: 24px;
  opacity: 0.7;
}
